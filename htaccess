
RewriteEngine on
RewriteCond %{REQUEST_URI} ^/index\.html$
RewriteRule ^(.*)$ https://themagicmonks.com/ [R=301,L]
RewriteCond %{HTTP_HOST} ^www\.ripurajagro\.com$ [NC]
RewriteRule ^(.*)$ https://themagicmonks.com/$1 [R=301,L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]


